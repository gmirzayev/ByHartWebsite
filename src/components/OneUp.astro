---
import "@styles/utils.css";
import Image from "astro/components/Image.astro";
const {src, alt=" ", class:classes} = Astro.props
---

<div class:list={["one-up", classes]}>
    <Image src={src} alt={alt}/>
</div>

<style>
    .one-up{
        padding:0 20px;

        img{
            width:100%;
            margin:0 auto;
        }
    }
</style>